class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int left = 0, right = 1;
        int profit = 0;
        while(right < prices.size()){
            if(prices[left] < prices[right]){
                int profitM = prices[right] - prices[left];
                profit = max(profit, profitM);
            }
            else{
                left = right;
            }
            right++;
        }
        return profit;
        
    }
};
