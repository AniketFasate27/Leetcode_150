class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        prod, cnt = 1, 0
        for num in nums:
            if num:
                prod = prod * num
            else:
                cnt = cnt + 1

        if cnt > 1:
            return [0]*len(nums)

        res = [0] * len(nums)
        for i, num in enumerate(nums):
            if cnt:
                if num == 0:
                    res[i] = prod
                else:
                    res[i] = 0

            else:
                res[i] = prod // num

        return res
        
