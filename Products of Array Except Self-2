class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        prod, zerocnt = 1,0
        for num in nums:
            if num:
                prod = prod * num
            else:
                zerocnt +=1

        if zerocnt > 1:
            return [0] * len(nums)

        res = [0] * len(nums)

        for i, j in enumerate(nums):
            if zerocnt > 0:
                if j == 0:
                    res[i] = prod
                else:
                    res[i] = 0

            else:
                res[i] = prod // j

        return res

        
